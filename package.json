{
  "name": "discord_deploy",
  "version": "1.0.16",
  "readme": "./README.md",
  "repository": {
    "url": "https://github.com/felipee-monteiro/discord_deploy"
  },
  "bin": {
    "discord_deploy": "./bin/cli.js"
  },
  "files": [
    "./bin/cli.js",
    "LICENSE",
    "README.md"
  ],
  "description": "A CLI to deploy slash guild commands easily.",
  "main": "./bin/cli.js",
  "type": "module",
  "scripts": {
    "dev": "npx tsc -w",
    "test:e2e": "npx c8 ava __tests__/e2e.test.js",
    "test:unit": "set NODE_OPTIONS=--experimental-vm-modules && set NODE_ENV=test && npx jest unit.test.js",
    "lint": "eslint .",
    "prod": "node bin\\cli.js deploy",
    "build": "tsc && npx tsup-node",
    "prepack": "npm run build"
  },
  "keywords": [
    "slash commands",
    "discord",
    "cli",
    "bot",
    "deploy",
    "commands",
    "discord.js",
    "eris"
  ],
  "tsup": {
    "entry": [
      "cli.ts"
    ],
    "target": "es2020",
    "outDir": "./bin",
    "format": "esm",
    "minify": true,
    "splitting": false,
    "sourcemap": false,
    "clean": true
  },
  "author": "felipee-monteiro",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/lodash.foreach": "^4.5.7",
    "@types/lodash.map": "^4.6.13",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/normalize-path": "^3.0.0",
    "ansi-styles": "^6.2.1",
    "ava": "^5.1.1",
    "cac": "^6.7.14",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.11",
    "eslint": "^8.32.0",
    "fast-glob": "^3.2.12",
    "jest": "^29.4.1",
    "lodash.foreach": "^4.5.0",
    "lodash.map": "^4.6.0",
    "node-fetch": "^3.3.0",
    "normalize-path": "^3.0.0",
    "ora": "^6.1.2",
    "prettier": "2.8.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^5.0.2"
  }
}
